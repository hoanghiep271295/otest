.divSubjectContent {
    white-space: pre-wrap;
    width: 350px;
    text-align: left;
    height: 40px;
    line-height: 40px;
    background-position: -4px -529px;
    color: #c75959;
    font-size: 18px;
    margin-top: 5px;
    margin-right: -11px;
    border: solid 1px;
    padding-left: 10px;
    border-radius: 10px;
}
.divSubjectContent:hover {
    background-color: #23b92f;
    transform: scale(1.2);
    -ms-transform: scale(1.2);
    -webkit-transform: scale(1.1);
}

.divSubjectContent a {
     text-decoration: none;
}

