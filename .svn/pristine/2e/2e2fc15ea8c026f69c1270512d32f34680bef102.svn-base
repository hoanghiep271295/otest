.flex-container {
    display: -webkit-flex;
    display: flex;
    width: 100%;
    height: auto;
    -ms-flex-direction:column;
    -o-flex-direction:column;
    -webkit-flex-direction:column;
    flex-direction:column;
}

.flex-item {
    width: auto;
    height: auto;
    margin: 7px;
}
.examform {
    height: 100%;
    background-color: #fffaf0;
    max-width: 85%;
    margin: auto;
    overflow-y: scroll;
    padding-left: 12px; 
    padding-right: 12px; 
}
.formexam {
    padding: 0 8px;
    box-shadow: 1px 1px 11px 2px #888888;
}
.questiongroup {
    margin-bottom: 15px;
    width: 95%;
    margin-top: 15px;
}
.btnmarginright {
    margin-right: 10px;
}
.textareawriting {
    margin: auto;
    display: block;
    width: 700px;
    width: 87%;
    min-height: 130px;
    font-family: Times New Roman, serif;
    font-size: 14.0pt;
    padding: .5rem .75rem;
    line-height: 1.25;
    color: #464a4c;
    background-color: #fff;
    word-wrap: break-word;
}
.btn-group:hover {
    background-color: white;
    color: black;
    border: 1px solid #e7e7e7;
}
.fieldsetGroup {
    padding: 20px 20px 10px;
    margin-right: 0;
    border: 1px solid #c0c0c0;
    clear: both;
    margin-left: 0;
    margin-bottom: 20px;
    margin: 0 2px;
    background-color: #f1f1f1;
    box-shadow: 0 2px 4px 0 #000000,0 2px 10px 0 #000000!important;
    box-shadow: 0 2px 4px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12)!important;
}
.btnSubmit {
        color: #FFFFFF;
    background-color: #4CAF50;
}
.btnSubmit:hover {
    color: #FFFFFF;
    box-shadow: 0 8px 16px 0 #000000, 0 6px 20px 0 #000000;
    box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.19);
    outline-width: 0
}

.inputRW {
    margin-left: 1%;
    min-width: 80%;
    padding: 5px;
    border-top: none;
    border-left: none;
    border-right: none;
    border-bottom: 0.5px dashed;
    
}
.inputFG {
    width: 150px;
    border-left: none;
    border-right: none;
    border-top: none;
    border-bottom: 0.5px dashed;
    background-color: white;
    margin-left: 2px;
    padding: 3px;
    margin-right: 2px;
}
#clockdiv{
	font-family: sans-serif;
	color: #fff;
	display: inline-block;
	font-weight: 100;
	text-align: center;
	font-size: 30px;
}
#clockdiv > div {
    margin: 1px;
	display: inline-block;
}

#clockdiv div > span {
    padding: 15px;
    -ms-border-radius: 3px;
    border-radius: 3px;
	background: #00816A;
	display: inline-block;
}
.boxFI {
    display: inline-block;
    min-width: 120px;
    margin-left: 2px;
    height: 25px;
    margin-right: 2px;
    border-bottom: 1px solid black;
    background-color: #f5edb2;
}
.contentsFI {
    width: 100px;
    height: 20px;
}
.spanFI {
	font-size:20px;
	line-height: 20px;	
	margin-left:20%;
	vertical-align: middle;	
}

.gridContent {
  display: grid;
  background-color: #3CE9FF;
  /* ReSharper disable once CssNotResolved */
  grid-gap: 0.75rem;
  grid-template: repeat(3, 1fr) / repeat(3, 1fr);
   padding: 20px 20px 10px;
  grid-template-columns:  fit-content(300px) / fit-content(300px) 
                          minmax(min-content, 300px) minmax(300px,
                          max-content) minmax(min-content, 
                          max-content) 1fr 1fr 1fr auto 300px;
                          width: 100%;
                          background-color: #3CE9FF;
                          list-style: none;
                          margin: 0;
                          padding: 0.75rem;
}

li.box {
  background-color: #a5ffa5;
}
p,div.contentsFI {
  margin: 1rem 0.5rem;
}
* {box-sizing: border-box;}